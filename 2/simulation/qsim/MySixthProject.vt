		B[0] = 1'b0;
		B[0] = #40000 1'b1;
		# 40000;
	end
	B[0] = 1'b0;
end 

Adder_vlg_sample_tst tb_sample (
	.A(A),
	.B(B),
	.sampler_tx(sampler)
