module Adder_vlg_vec_tst();
// constants                                           
// general purpose registers
reg [7:0] A;
reg [7:0] B;
// wires                                               
wire Cout;
wire [7:0] Sum;

wire sampler;                             

// assign statements (if any)                          
Adder i1 (
// port map - connection between master ports and signals/registers   
	.A(A),
	.B(B),
	.Cout(Cout),
	.Sum(Sum)
);
// A[ 7 ]
initial
begin
	A[7] = 1'b0;
end 
// A[ 6 ]
initial
begin
	A[6] = 1'b0;
end 
// A[ 5 ]
initial
begin
	A[5] = 1'b0;
end 
// A[ 4 ]
initial
begin
	A[4] = 1'b0;
end 
// A[ 3 ]
initial
begin
	repeat(3)
	begin
		A[3] = 1'b0;
		A[3] = #160000 1'b1;
		# 160000;
	end
	A[3] = 1'b0;
end 
// A[ 2 ]
initial
begin
	repeat(6)
	begin
		A[2] = 1'b0;
		A[2] = #80000 1'b1;
		# 80000;
	end
	A[2] = 1'b0;
end 
// A[ 1 ]
initial
begin
	repeat(12)
	begin
		A[1] = 1'b0;
		A[1] = #40000 1'b1;
		# 40000;
	end
	A[1] = 1'b0;
end 
// A[ 0 ]
always
begin
	A[0] = 1'b0;
	A[0] = #20000 1'b1;
	#20000;
end 
// B[ 7 ]
initial
begin
	B[7] = 1'b0;
end 
// B[ 6 ]
initial
begin
	B[6] = 1'b0;
end 
// B[ 5 ]
initial
begin
	B[5] = 1'b0;
end 
// B[ 4 ]
initial
begin
	B[4] = 1'b0;
end 
// B[ 3 ]
initial
begin
	B[3] = 1'b0;
	B[3] = #320000 1'b1;
	B[3] = #320000 1'b0;
	B[3] = #320000 1'b1;
end 
// B[ 2 ]
initial
begin
	repeat(3)
	begin
		B[2] = 1'b0;
		B[2] = #160000 1'b1;
		# 160000;
	end
